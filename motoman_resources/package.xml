<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>motoman_resources</name>
  <version>0.0.0</version>
  <description>
    <p>Shared configuration data, 3D models, launch files materials, etc. for motoman robots.</p>
    <p>
      This package contains shared configuration data, 3D models, launch files
      materials, etc. that are shared between different Motoman robot packages.
    </p>
    <p>
      Approach inspired by the fanuc_resources package. See https://github.com/ros-industrial/fanuc.
    </p>
  </description>

  <!-- The original author is Alec Tiefenthal -->
