  <maintainer email="g.a.vanderhoorn@tudelft.nl">G.A. vd. Hoorn (TU Delft Robotics Institute)</maintainer>
  <maintainer email="alec@path-robotics.com">Alec Tiefenthal (Path Robotics)</maintainer> -->
  <author>tc-huang</author>
  <maintainer email="tzu.chun.huang.tw@gmail.com">tc-huang</maintainer>
  <license>BSD-3-Clause</license>

  <url type="website">https://github.com/tc-huang/motoman</url>
  <url type="bugtracker">https://github.com/tc-huang/motoman</url>
  <url type="repository">https://github.com/tc-huang/motoman</url>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
